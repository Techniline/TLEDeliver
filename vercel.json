{
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },

    { "source": "/assets/:path*", "destination": "/app/assets/:path*" },
    { "source": "/login", "destination": "/app/index.html" },
    { "source": "/dashboard", "destination": "/app/index.html" },

    { "source": "/", "destination": "/app/index.html" },
    { "source": "/:path*", "destination": "/app/index.html" }
  ]
}
